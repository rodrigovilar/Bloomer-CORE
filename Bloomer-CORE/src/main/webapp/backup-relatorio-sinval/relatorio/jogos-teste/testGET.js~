$(document).ready(function(){
	ajax("http://localhost:8080/Bloomer-CORE/jogos");
});

function ajax(path){

	$.ajax({
		//type: "GET",
		url: path,
		//data: {nome: value},
		dataType: "json",
		success: function(json){
			alert(json)
			var options = json.nomeJogo;

			$("#content").html(options);
		}
	});
};
